{
  "name": "dart-analysis-panel",
  "displayName": "Dart Analysis Panel",
  "description": "Custom panel to view Dart/Flutter analysis results in VSCode/Cursor similar to Android Studio. Displays issues grouped by file with severity indicators and click-to-navigate functionality.",
  "version": "0.0.1",
  "publisher": "fbernaly",
  "repository": {
    "type": "git",
    "url": "https://github.com/fbernaly/dart-analysis-panel.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dartAnalysisPanel.show",
        "title": "Show Panel",
        "category": "Dart Analysis",
        "icon": "$(checklist)"
      },
      {
        "command": "dartAnalysisPanel.refresh",
        "title": "Refresh",
        "category": "Dart Analysis",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dartAnalysisPanel.refresh",
          "when": "webviewId == dartAnalysisView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
